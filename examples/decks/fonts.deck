{script:sheet.2}
on click do
 f:font.event.currfont
 ws:workzone.size
 tc:card.add["canvas"]
 tc.size:ws
 c:floor ws[0]/f.size[0]
 each i in range 256-32
  g:f[32+i]
  p:f.size*(c%i),(floor i/c)
  tc.paste[g p]
 end
 card.image.paste[tc.copy[] workzone.pos]
