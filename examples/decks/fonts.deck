{script:diacritics.0}
classes:insert k v top with
 "acute"        "" dict "AEIOUYaeiouyCcNnSsZz" 1
 "grave"        ""           dict "AEIOUaeiou"           1
 "circumflex"   ""           dict "AEIOUaeiou"           1
 "caron"        ""                 dict "SsZz"                 1
 "umlaut"       ""         dict "AEIOUaeiouyY"         1
 "hungarumlaut" ""                 dict "OoUu"                 1
 "tilde"        ""               dict "ANOano"               1
 "macron"       ""           dict "AaEeIiOoUu"           1
 "ring"         ""                   dict "Aa"                   1
 "breve"        ""                   dict "Aa"                   1
 "overdot"      ""                   dict "Zz"                   1
 "ogonek"       ""                 dict "AaEe"                 0
 "cedilla"      ""                   dict "Cc"                   0
 "comma"        ""                 dict "SsTt"                 0
 "other"        ""     dict "AaCc6BBDOoLliC!?"     1
end

on view do
 c:(raze classes)[class.value]
 k:" " fuse keys c
 v:" " fuse range c
 ref.text:"as seen in: %s\n  based on: %s" format (list k),(list v)

 f:font.event.currfont
 present.toggle["solid" f[first keys c].size[0]]

 s:floor workzone.size[1]/f.size[1]
 editor.lsize:f.size
 editor.scale:s
 editor.lsize:f.size
 editor.scale:s
 editor.pos:workzone.pos+.5*workzone.size-editor.size
 editor.toggle["solid" !class.value~"other"]
 view_preview[]
end

on view_preview do
 f:font.event.currfont
 d:class.value
 i:editor.copy[]
 x:2
 y:floor .5*preview.size[1]-f.size[1]
 preview.clear[]
 each o c in (raze classes)[class.value]
  g:composite[f d o c i]
  preview.paste[g x,y]
  x:x+g.size[0]+2
 end
end

on composite f dia orig char img do
 r:f[orig]
 p:0,0
 if center.value
  b:img.bounds
  p:((.5*r.size[0]-b.size[0])-b.pos[0]),0
 end
 r.paste[img p 1]
 r
end
{end}

