{script:sheet.3}
on click do
 f:font.event.currfont
 h:font.size[1]
 s:card.image.copy[workzone.pos workzone.size]
 c:floor workzone.size[0]/f.size[0]

 g:each i in 1+range 256-33
  p:f.size*(c%i),(floor i/c)
  s.copy[p f.size].map[1 dict 1 0]
 end
 
 di:15+range 10
 digits:g@di
 db:digits..bounds
 dw:max db..size[0]

 if !monospace.value
  each glyph i in g
   b:glyph.bounds
   if monodigits.value & i in di
    n:glyph.copy[b.pos[0],0 b.size[0],h]
    glyph.size:dw,n.size[1]
    glyph.map[() 0]
    glyph.paste[n (floor .5*dw-n.size[0]),0]
   else
    n:glyph.copy[0,0 (b.size[0]+b.pos[0]),h]
    glyph.size:n.size
    glyph.paste[n]
   end
  end
 end
 
 each glyph i in g
  if (list 0)~keys glyph.hist
   glyph.size:0,0
  end
 end
 
 each glyph i in g f[i+33]:glyph end
end
{end}

