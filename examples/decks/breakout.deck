{contraption:ball}
size:[20,20]
margin:[0,0,0,0]
description:"a breakout ball that moves based on its velocity and thuds off other contraptions it overlaps."
image:"%%IMG0ABQAFAH4AA//AB//gD//wH//4H//4H//4P//8P//8P//8P//8P//8P//8H//4H//4H//4D//wB//gA//AAH4AA=="
script:"ball.0p"
attributes:{"name":[],"label":[],"type":[]}
{widgets}
v:{"type":"field","size":[44,20],"pos":[39,-19],"locked":1,"animated":1,"value":"[0,0]"}

{script:ball.0p}
on get_vel do v.data end
on set_vel x do v.data:x end
on view do
 c:card.pos+.5*card.size
 n:get_vel[]+c
 p:card.parent
 thuds:0,0
 each w in card.name drop p.widgets
  if min (n>w.pos)&(n<w.pos+w.size)
   if w.thud
    b:!(c>w.pos)&(c<w.pos+w.size)
    if 2=sum b b:0,1 end # prefer vertical bounces
    thuds:thuds|b
   end
  end
 end
 if max (n<0)|(n>p.size)
  card.parent.event.reset
 else
  set_vel[get_vel[]*(1,-1)@thuds]
  card.pos:get_vel[]+card.pos
 end
end
{end}

