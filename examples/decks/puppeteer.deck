{script:customcommands.0}
on command x do
 if x~"light"
  o.speed:2
  o.fcolor:colors.black
  o.bcolor:colors.white
  dd.style[o]
 elseif x~"dark"
  o.speed:2
  o.fcolor:colors.white
  o.bcolor:colors.black
  dd.style[o]
 else
  pt.command[deck x]
 end
end
{end}

