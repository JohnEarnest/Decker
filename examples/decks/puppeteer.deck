{module:pt}
description:"Display and animate sprites for visual novels."
version:1.3
{script}

local FRONT:999999  # foremost (reasonable) widget index
local DELAY:30      # default delay in frames
local host  :0      # the deck
local target:0      # the (current) target card
local talk_enable:0 # should we be animating 'talking' puppets currently?

on eval_snippet x do
 local d:()
 d.deck    :host
 d.patterns:host.patterns
 d.card    :host.card
 d:d       ,host.modules..value
 d:d       ,host.cards
 d:d       ,host.card.widgets
 eval[x d 1]
end
on tween a b t    do (b*t)+(a*1-t) end
on blink_interval do 150+random[150] end

#######################################
#
#  State Management
#
#######################################

on get_charsheet name do
 host.cards[name]
end

on get_state_name name do
 "%s_pstate" format name
end

on puppet_names do
 extract where target.widgets @ get_state_name @ value from keys target.widgets
end

on init_puppet name do
 sheet:get_charsheet[name]
 if !name in target.widgets
  if "contraption"~typeof first sheet.widgets
   pc:first target.paste[sheet.copy[first sheet.widgets]]
   pc.name:name
  else
   pc:target.add["canvas" name]
   pc.border:0
   pc.locked:1
   pc.animated:1
   pc.script:"
    on view do
     pt.animate[deck me.name]
    end
   "
  end
  pc.show:"transparent"
  target.widgets.dd_overlay.index:FRONT
 end
 sname:get_state_name[name]
 if !sname in target.widgets
  ps:target.add["field" sname]
  ps.show:"none"
  s:()
  s.emote:first keys sheet.widgets
  s.hflip:0
  s.pos:"center"
  s.blink:blink_interval[]
  ps.data:s
 end
end

on get_puppet name do
 init_puppet[name]
 r:target.widgets[get_state_name[name]].data
 r.wid:target.widgets[name]
 r.name:name
 r
end

on set_puppet state do
 init_puppet[state.name]
 target.widgets[get_state_name[state.name]].data:("wid","name") drop state
end

#######################################
#
#  Presentation
#
#                                offtop
#               +------------------------------------------+
#    offtopleft | topleft          top            topright | offtopright
#               |                                          |
# offcenterleft | centerleft      center       centerright | offcenterright
#               |                                          |
#       offleft | left            bottom             right | offright
#               +------------------------------------------+
#                 offbottomleft  offbottom  offbottomright
#
#######################################

positions:raze insert key value with
 "center"         on center         c do .5*target.size-c.size end
 "offcenterleft"  on offcenterleft  c do (first -c.size           ),(.5*last target.size-c.size) end
 "centerleft"     on centerleft     c do (0                       ),(.5*last target.size-c.size) end
 "centerright"    on centerright    c do (first target.size-c.size),(.5*last target.size-c.size) end
 "offcenterright" on offcenterright c do (first target.size       ),(.5*last target.size-c.size) end
 "offtopleft"     on offtopleft     c do (first -c.size    ),0 end
 "offtopright"    on offtopright    c do (first target.size),0 end
 "topleft"        on topleft        c do (0                          ),0 end
 "offtop"         on offtop         c do (.5*first target.size-c.size),(last -c.size) end
 "top"            on top            c do (.5*first target.size-c.size),0 end
 "topright"       on topright       c do (   first target.size-c.size),0 end
 "offleft"        on offleft        c do (   first            -c.size),(last target.size-c.size) end
 "left"           on left           c do (0                          ),(last target.size-c.size) end
 "bottom"         on bottom         c do (.5*first target.size-c.size),(last target.size-c.size) end
 "right"          on right          c do (   first target.size-c.size),(last target.size-c.size) end
 "offright"       on offright       c do (   first target.size       ),(last target.size-c.size) end
 "offbottomleft"  on offbottomleft  c do (0                          ),(last target.size       ) end
 "offbottom"      on offbottom      c do (.5*first target.size-c.size),(last target.size       ) end
 "offbottomright" on offbottomright c do (   first target.size-c.size),(last target.size       ) end
end

on get_pos canv pos do
 if pos in positions # predefined position
  positions[pos][canv]
 elseif pos in target.widgets # custom position (centered)
  wid:target.widgets[pos]
  (wid.pos+.5*wid.size)-.5*canv.size
 else
  positions.bottom[canv]
 end
end

skews:raze insert key value with
 "offcenterleft" (-1, 0)
 "centerleft"    (-1, 0)
 "center"        ( 0, 0)
 "centerright"   ( 1, 0)
 "offcenterright"( 1, 0)
 "offtopleft"    (-1,-1)
 "offtopright"   ( 1,-1)
 "topleft"       (-1,-1)
 "offtop"        ( 0,-1)
 "top"           ( 0,-1)
 "topright"      ( 1,-1)
 "offleft"       (-1, 1)
 "left"          (-1, 1)
 "bottom"        ( 0, 1)
 "right"         ( 1, 1)
 "offright"      ( 1, 1)
 "offbottomleft" (-1, 1)
 "offbottom"     ( 0, 1)
 "offbottomright"( 1, 1)
end

on paint_puppet state do
 if "contraption"~typeof state.wid
  if !"none"~state.wid.show state.wid.animate[] end
 else
  sheet:get_charsheet[state.name]
  face:sheet.widgets[state.emote]
  if state.talking&talk_enable
   faces:face,extract value where (key like ("%s_open*" format state.emote))&!(key like "*_blink") from sheet.widgets
   face:faces[(count faces)%floor sys.frame/6]
  elseif state.blink
   face:face unless sheet.widgets["%s_blink" format state.emote]
  end
  canv:state.wid
  canv.size:face.size
  if !"none"~canv.show
   img:if state.hflip face.copy[].transform["horiz"] else face.copy[] end
   if (state.anim~"backforth")&2%floor sys.frame/15 img.transform["horiz"] end
   canv.clear[]
   if state.anim~"breathe"
    skew:skews[state.pos]
    m:3
    n:m*1+sin(pi+.1*canv.index)*(sys.frame)/120
    p:(n+2*m*skew[0]),0
    s:(canv.size[0]-2*n),canv.size[1]
    canv.paste[img p,s]
   elseif state.anim~"spin"
    tmp:image[img.size*2]
    tmp.paste[img .5*tmp.size-img.size]
    tmp.rotate[sys.frame/120]
    canv.paste[tmp .5*canv.size-tmp.size]
   else
    canv.paste[img]
   end
  end
 end
end

on move_puppet state do
 canv:state.wid
 canv.pos:if state.steps
  tween[get_pos[canv state.gpos] get_pos[canv state.pos] state.steps/state.delay]
 else
  get_pos[canv state.pos]
 end
 skew:skews[state.pos]
 if state.anim~"bob"
  m:10&0.1*canv.size[1]
  n:m*sin(pi+.1*canv.index)*sys.frame/60
  canv.pos:canv.pos + 0,(n+m*skew[1])
 end
 if state.anim~"shake"
  m:.05*canv.size[0]
  n:m*cos(pi+.1*canv.index)*sys.frame/5
  canv.pos:canv.pos + (n+m*skew[0]),0
 end
end

on exec_anim state do
 pending_paint:0
 pending_move :0
 # blinking
 state.blink_delay:0|state.blink_delay-1
 if !state.blink_delay
  state.blink:4 # blink for 4 frames!
  state.blink_delay:blink_interval[]
  pending_paint:1
 elseif state.blink
  state.blink:0|state.blink-1
  if !state.blink pending_paint:1 end
 end
 if state.talking&talk_enable pending_paint:1 end
 # movement tweening
 if state.steps
  state.steps:0|state.steps-1
  if !state.steps state.pos:state.gpos end
  pending_move:1
 end
 # idle animations
 if state.anim in "breathe","spin","backforth" pending_paint:1 end
 if state.anim in "bob","shake" pending_move:1 end
 # wrap up
 if "contraption"~typeof state.wid pending_paint:1 end
 if pending_paint              paint_puppet[state] end
 if pending_paint|pending_move move_puppet[state] end
 set_puppet[state]
end

#######################################
#
#  Commands
#
#  show  NAME               # default emote, pos is center OR kept as-is
#  show  NAME EMOTE         # pos is center
#  show  NAME EMOTE POS
#  hide  NAME
#  flip  NAME 1|0           # flip sprite image horizontal
#  order NAME front|back    # change sprite order
#  move  NAME POS DELAY     # interpolated; delay is in frames @ 60fps
#  anim  NAME TYPE          # (none, bob, shake, breathe)
#  wait                     # blocking wait for all in-progress anims to finish
#  wait DELAY               # blocking wait for exactly N frames
#  shake XMAG YMAG DELAY    # screen shake for N frames with horizontal/vertical intensity
#  talk NAME...             # select 0 or more 'talking' puppets by name
#  play NAME                # play a sound
#  lil ANYTHING             # execute a lil snippet
#  clear                    # remove all puppets
#
#######################################

commands.show:on cmd_show x do
 if count x
  state:get_puppet[first x]
  if x[1] state.emote:x[1] state.wid.emote:x[1] end
  if x[2] state.pos  :x[2] end
  state.wid.show:"transparent"
  paint_puppet[state]
  move_puppet[state]
  set_puppet[state]
 else explode["error: missing puppet name for !show"] end
end

commands.hide:on cmd_hide x do
 if count x
  get_puppet[(first x)].wid.show:"none"
 else alert["error: missing puppet name for !hide"] end
end

commands.flip:on cmd_flip x do
 if count x 
  state:get_puppet[first x]
  state.hflip:0+if 1<count x x[1] else !state.hflip end
  state.wid.flip:state.hflip
  paint_puppet[state]
  set_puppet[state]
 else alert["error: missing puppet name for !flip"] end
end

commands.order:on cmd_order x do
 if count x
  get_puppet[(first x)].wid.index:if "back"~x[1] -1 else FRONT end
  target.widgets.dd_overlay.index:FRONT
 else alert["error: missing puppet name for !order"] end
end

commands.move:on cmd_move x do
 if count x
  dest:x[1]
  delay:DELAY unless x[2]
  state:get_puppet[first x]
  state.gpos:dest
  state.delay:delay
  state.steps:delay
  move_puppet[state]
  set_puppet[state]
 else alert["error: missing puppet name for !move"] end
end

commands.anim:on cmd_anim x do
 if count x
  state:get_puppet[first x]
  state.anim:x[1] state.wid.anim:state.anim
  set_puppet[state]
 else alert["error: missing puppet name for !anim"] end
end

commands.wait:on cmd_wait x do
 local ol:target.widgets.dd_overlay
 local sh:ol.show
 ol.show:"none"
 on tick do target.event["animate"] sleep[] end
 if first x
  each in range 0+first x tick[] end
 else
  while max (get_puppet @ puppet_names[])..steps tick[] end
 end
 ol.show:sh
end

local shake_mag:30,30
local shake_dur:15
commands.shake:on cmd_shake x do
 if 1<count x shake_mag:0|0+2 take x end
 if 2<count x shake_dur:1|0+x[2] end
 local ol:target.widgets.dd_overlay
 local sh:ol.show
 ol.show:"none"
 go[host.card on shake_trans c a b t do
  local m:shake_mag*1-t
  c.pattern:1
  c.rect[0,0 c.size]
  c.paste[a (random@2*m)-m]
 end shake_dur]
 ol.show:sh
end

commands.talk:on cmd_talk x do
 each name in puppet_names[]
  state:get_puppet[name]
  state.talking:name in x
  set_puppet[state]
 end
end

commands.play:on cmd_play x do
 play[first x]
end

commands.clear:on cmd_clear do
 clear[target]
end

on exec_command text do
 if "lil"~3 limit text
  eval_snippet[3 drop text]
 else
  f:extract where count@value from " " split text
  (commands[first f])[1 drop f]
 end
end

#######################################
#
#  Public API
#
#######################################

module.setup:on setup deck card commands do
 host:deck
 target:card
 # accept list of commands OR a single string with newlines,
 # with or without a leading '!'
 each line in "\n" split "\n" fuse (),commands
  if "!"~first line line:1 drop line end
  exec_command[line]
 end
end

module.command:on command deck commands do
 setup[deck deck.card commands]
end

module.animate:on animate deck puppet do
 host:deck
 target:deck.card
 if puppet
  exec_anim[get_puppet[puppet]]
 else
  each name in puppet_names[]
   exec_anim[get_puppet[name]]
  end
 end
end

module.clear:on clear card do
 target:host.card unless card
 each name in puppet_names[]
  target.remove[target.widgets[name]]
  target.remove[target.widgets[get_state_name[name]]]
 end
end

module.talk:on talk enable do
 talk_enable:enable
 each name in puppet_names[]
  state:get_puppet[name]
  state.wid.talk:enable
  set_puppet[state]
  paint_puppet[state]
 end
end

{end}

{module:dd}
description:"Display sequential, styled modal dialogs for visual novels."
version:1.7
{script}
# state/constants:
local host:0
local canvas:0
local padding:10
local margin:20
local arrow:image["%%IMG0AAsABv/gf8A/gB8ADgAEAA=="]
local arrow_anim:arrow,image[arrow.size]

# config:
local tfont:"body"
local bfont:"body"
local align:"left"
local balign:"center"
local fcolor:32
local bcolor:1
local next:arrow_anim
local nextd:10
local speed:0
local names:() dict ()
local border:0
local bborder:0
local pad:()
local bpad:()
local osound:"" # open box
local nsound:"" # advance to next box
local asound:"" # make a choice
local tsound:"" # talk
local dsize:()
local dpos:0

on ifmt x do x format names end
on fmt x do update text:ifmt @ text from rtext.cat[x] end
on tween a b t do a+(b-a)*0|1&t end
on rin x pos size do min(x>pos),(x<pos+size) end
on tick do host.card.event["animate"] sleep[] end

on sfx x do
 play[if "list"~typeof x random[x] else x end]
end

on draw_border do
 canvas.clear[]
 if border.type~"image"
  canvas.segment[border 0,0,canvas.size (range pad)]
 else
  canvas.pattern:bcolor
  canvas.rect[0,0 canvas.size]
  canvas.pattern:fcolor
  canvas.box[0,0 canvas.size]
 end
end

on repos pos size do
 if dpos~"center"
  .5*host.card.size-size
 elseif dpos in host.card.widgets
  t:host.card.widgets[dpos]
  t.pos+.5*t.size-size
 else pos
 end
end

on resize height do
 cs0:canvas.size
 cs1:((host.card.size[0]-2*margin) unless dsize[0]),height
 cp0:canvas.pos
 cp1:repos[(.5*host.card.size[0]-cs1[0]),host.card.size[1]-cs1[1]+margin cs1]
 each t in .2*range 6
  canvas.size:tween[cs0 cs1 t]
  canvas.pos :tween[cp0 cp1 t]
  draw_border[]
  tick[]
 end
end

module.open:on open deck etc do
 if canvas & canvas.show~"transparent" close[] end
 host:deck
 canvas:host.card.widgets.dd_overlay
 if !canvas canvas:host.card.add["canvas" "dd_overlay"] end
 style[etc]
 canvas.size:((host.card.size[0]-2*margin) unless dsize[0]),0
 canvas.pos:repos[(.5*host.card.size[0]-canvas.size[0]),host.card.size[1]-margin canvas.size]
 canvas.show:"transparent"
 canvas.border:0
 sfx[osound]
end

module.style:on style etc do
 fcolor:if "fcolor" in etc etc.fcolor else 32         end
 bcolor:if "bcolor" in etc etc.bcolor else 1          end
 tfont :if "tfont"  in etc etc.tfont  else "body"     end
 bfont :if "bfont"  in etc etc.bfont  else "body"     end
 align :if "align"  in etc etc.align  else "left"     end
 balign:if "balign" in etc etc.balign else "center"   end
 names :if "names"  in etc etc.names  else ()dict()   end
 next  :if "next"   in etc etc.next   else arrow_anim end
 nextd :if "nextd"  in etc etc.nextd  else 10         end
 speed :if "speed"  in etc etc.speed  else 0          end
 osound:if "osound" in etc etc.osound else ""         end
 nsound:if "nsound" in etc etc.nsound else ""         end
 asound:if "asound" in etc etc.asound else ""         end
 tsound:if "tsound" in etc etc.tsound else ""         end
 dsize: if "size"   in etc etc.size   else ()         end
 dpos:  if "pos"    in etc etc.pos    else 0          end
 if !dsize[0] dsize[0]:nil end
 if !dsize[1] dsize[1]:nil end
 border:etc.border.image.copy[]
 pad:"ltrb" dict 4 take padding unless etc.border.margin
 bborder:etc.bborder.image.copy[]
 bpad:"ltrb" dict 4 take (2,10,2,10) unless etc.bborder.margin
 if balign~"left"  balign:"center_left"  end
 if balign~"right" balign:"center_right" end
 if "image"~typeof next next:next,image[next.size] end
 if (()~next)|(!"list"~typeof next) next:arrow_anim end
 if 1~count next next:next,image[next.size] end
 nextd:1|nextd
end

module.getstyle:on getstyle do
 raze insert k v with
  "fcolor"  fcolor
  "bcolor"  bcolor
  "tfont"   tfont
  "bfont"   bfont
  "align"   align
  "balign"  (("center_left","center_right","center") dict ("left","right","center"))[balign]
  "names"   names
  "next"    next
  "nextd"   nextd
  "speed"   speed
  "osound"  osound
  "nsound"  nsound
  "asound"  asound
  "tsound"  tsound
  "size"    dsize
  "pos"     dpos
  "border"  (("image","margin")dict((list  border),(list range  pad)))
  "bborder" (("image","margin")dict((list bborder),(list range bpad)))
 end
end

module.show:on show x do
 canvas.toggle["transparent" x]
end

module.close:on close do
 if canvas & canvas.show~"transparent" resize[0] end
 host.card.remove[canvas]
 host:canvas:0
end

module.say:on say text do
 text:fmt @ rtext.split["\n\n" text]
 canvas.font:tfont
 each seg i in text
  t:rtext.string[seg]
  if t[0]~"!"
   host.card.event["command" (1 drop t)]
  else
   dim:canvas.textsize[seg (canvas.size[0] unless dsize[0])-(pad.l+pad.r)]
   szh:dim[1]+(pad.t+pad.b)+next.size[1]
   if dsize[0] dim[0]:dsize[0]-pad.l+pad.r end
   resize[szh unless dsize[1]]
   canvas.pattern:fcolor
   if speed
    host.modules.pt.value.talk[1]
    splits: extract index,1+count t where value in (" ","\n") from "" split t
    si:0 db:0 while si<count splits
     sfx[tsound]
     each in range speed
      if pointer.down si:-1+count splits db:1 end
      tick[]
     end
     draw_border[]
     canvas.pattern:fcolor
     canvas.text[rtext.span[seg 0,splits[si]] pad.l,pad.t,dim ("top_%s" format align)]
     si:si+1
    end
    host.modules.pt.value.talk[0]
    if db
     while pointer.held|pointer.down tick[] end
    end
   else
    canvas.text[seg pad.l,pad.t,dim ("top_%s" format align)]
   end
   ns:next..copy[].map[(0,1) dict bcolor,fcolor]
   on wait do
    a:ns[(count ns)%floor sys.frame/nextd]
    canvas.paste[a canvas.size-(pad.r,pad.b)+a.size]
    tick[]
   end
   while !pointer.held|pointer.down wait[] end
   sfx[nsound]
   while  pointer.held|pointer.up   wait[] end
  end
 end
end

module.ask:on ask text choices do
 seg:fmt[text]
 canvas.font:tfont
 dim:canvas.textsize[seg (canvas.size[0] unless dsize[0])-(pad.l+pad.r)]
 if 1>count choices choices:list "error: empty choice list for dd.ask!" end
 segs:fmt @ choices
 canvas.font:bfont
 dims:each choice in segs
  canvas.textsize[choice (canvas.size[0] unless dsize[0])-(bpad.t+bpad.b)+(pad.t+pad.b)][1]+(bpad.t+bpad.b)
 end
 szh:dim[1]+(pad.t+pad.b)+(sum dims)+(padding*count dims)
 if dsize[0] dim[0]:dsize[0]-pad.l+pad.r end
 resize[szh unless dsize[1]]
 local r:-1 while -1~r
  draw_border[]
  canvas.font:tfont
  canvas.pattern:fcolor
  canvas.text[seg (pad.l,pad.t),dim ("top_%s" format align)]
  c:dim[1]+pad.t+padding
  ow:canvas.size[0]-(pad.l+pad.r)
  each choice i in segs
   local rpos:pad.l,c
   local rsize:ow,dims[i]
   local tpos:rpos+bpad.l,bpad.t
   local tsize:rsize-((bpad.l+bpad.r),(bpad.t+bpad.b))
   # active background (invert label)
   canvas.pattern:fcolor
   if (pointer.held|pointer.down|pointer.up)&rin[pointer.pos-canvas.pos rpos rsize]
    if bborder.type~"image"
     canvas.rect[rpos rsize]
    else
     canvas.rect[rpos+2 rsize-4]
    end
    if pointer.up r:i sfx[asound] end
    canvas.pattern:bcolor
   end
   # label
   canvas.font:bfont
   canvas.text[choice tpos,tsize balign]
   # border
   if bborder.type~"image"
    canvas.segment[bborder rpos,rsize (range bpad)]
   else
    canvas.pattern:fcolor
    canvas.box[rpos rsize]
   end
   c:c+padding+dims[i]
  end
  tick[]
 end
 r
end

module.chat:on chat text choices do
 r:0
 st:getstyle[]
 while count choices
  k:keys choices
  style[st]
  v:ask[text k]
  vv:choices[k[v]]
  if "number"~typeof vv
   choices:()
   r:vv
  else
   choices:k[v] drop choices
   say[vv]
  end
 end
 r
end
{end}

{contraption:enum}
size:[100,25]
resizable:1
margin:[5,5,5,5]
description:"select from among enumerated string values."
script:"enum.0p"
template:"on change val do\n \nend"
attributes:{"name":["options"],"label":["Options"],"type":["code"]}
{widgets}
s:{"type":"slider","size":[100,25],"pos":[0,0],"interval":[0,100],"style":"compact"}
o:{"type":"field","size":[100,20],"pos":[0,-30],"locked":1,"style":"plain"}

{script:enum.0p}
on get_options   do o.text end
on get_value     do ("\n" split o.text)[s.value] end
on set_options x do o.text:x s.value:0 view[] end

on set_value x do
 v:"\n" split o.text
 d:v dict range count v
 s.interval:0,(count v)-1
 s.value:v[0] unless d[x]
 s.format:v[s.value]
 s.font:card.font
 s.show:card.show
 s.locked:card.locked
end

on change do
 view[]
 card.event["change" get_value[]]
end

on view do
 v:"\n" split o.text
 s.interval:0,(count v)-1
 s.format:v[s.value]
 s.font:card.font
 s.show:card.show
 s.locked:card.locked
end

{end}

{contraption:buggo}
size:[154,140]
margin:[0,0,0,0]
description:"it's an egg; it's a bug"
script:"buggo.0p"
{widgets}
lwing:{"type":"canvas","size":[122,122],"pos":[2,-2],"locked":1,"show":"transparent","border":0,"image":"%%","scale":1}
body:{"type":"canvas","size":[123,92],"pos":[15,34],"locked":1,"animated":1,"show":"transparent","border":0,"image":"%%","scale":1}
rwing:{"type":"canvas","size":[122,122],"pos":[22,1],"locked":1,"show":"transparent","border":0,"image":"%%","scale":1}
rw:{"type":"canvas","size":[122,122],"pos":[188,-68],"locked":1,"show":"none","border":0,"image":"%%","scale":1}
