{script:buggo.0p}

on set_anim  x do app.show["set buggo anim" x] end
on set_flip  x do app.show["set buggo flip" x] end
on set_talk  x do app.show["set buggo talk" x] end
on set_emote x do emote.text:x end

on view do
 lwing.paste[lw.copy[].rotate[-(pi/14)*sin 0.1*sys.frame]]
 rwing.paste[rw.copy[].rotate[ (pi/12)*sin 0.1*sys.frame]] 
 lf:lg.images@(0,0,0,0,0,1,2,3)
 legs.paste[lf[(count lf)%floor sys.frame/10]]
 ff:(faces.value.name dict faces.value.value)
 face.clear[]
 face.paste[image[ff[emote.text]]]
end

on get_animate do
 on animate do view[] end
end
{end}

