{script:wigglyModal.2p}
on oval pos size do
 flip pos+size*flip unit 2*pi*(range 20)/20
end

on shake obj tick do
  local st: 10 # time
  local sm: 15 # magnitude
  local sf:0.5 # fade
  local so:obj.pos
  while sm>0
    st:st-1
    obj.pos:so+random[sm,-sm 2]
    if st<0 sm:sm-sf end
    tick[]
  end
  obj.pos:so
end

on click do
 audio.boom
 backup[]
 trans.value:0
 c.show:"solid"
 local s:()
 shake[c on tick do
  c.pattern:0
  each v in s c.poly[v] end
  s:each _ in range 5
   oval[random[max c.size 2] 10+random[30]]
  end
  c.pattern:1
  each v in s c.poly[v] end
  sleep[]
 end]
 c.pattern:1
 fr.value:raze rtext.cat @ (3 take image[zone.size])..copy[]
end

{end}

{script:wigglyModal.3p}
on click do
 audio.erase
 t:hist.value
 hist.value:fr.value
 fr.value:t
end
{end}

{script:wigglyModal.4p}
