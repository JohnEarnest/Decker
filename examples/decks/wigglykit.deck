{script:wigglyModal.7p}
tw:(list -12,-6,-2,0),(list -2,-6,-12,-14)
on click do
 p:REST=tc..pos[0]
 i:sum(me=tc)*range count tc
 if p[i]
  audio.pick
  each ai in range 4
   each t ti in tc
    off:if !p[ti]
     (-12,-6,-2,0)[ai]
    elseif t~me
     (-2,-6,-12,-14)[ai]
    end
    t.pos:(REST+off),t.pos[1]
   end
   sleep[]
  end
  if me~canv_mask trans.value:1 end
 end
end

on drag pos do end
on release pos do end
{end}

