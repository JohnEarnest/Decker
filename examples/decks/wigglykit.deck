{script:wigglyModal.0p}
on get_animate do view end
on frames do fr.images end
REST:388
tc:canv_pen,canv_sumi,canv_stipple,canv_airbrush,canv_pink,canv_cyan,canv_yellow,canv_mask,canv_eraser
on toolval do
 5 drop first extract value..name,"canv_pen" where !REST=value..pos[0] from tc
end

on backup do hist.value:raze rtext.cat @ frames[]..copy[] end
delta:"%j"parse"[-2,-1,-1,0,1,1,2]"
marker:"%%IMG0ABUAFgD/gAP/wA//4B//8D//+H//+P//+P//+P//+P//+P//+P//+P//+P//8P//8P//4P//wH//gD//AB/+AAf8AAPgAA=="
on wiggle x do return random[delta  2]+x end
on get_init do
 on init do
  card.show:"transparent"
  locks.data:deck.card.widgets..locked
  deck.card.widgets..locked:1
 end
end
on cleanup do
 each v k in locks.data
  deck.card.widgets[k].locked:v
 end
 deck.remove[card]
end
on get_value do
 r.frames:frames[]
 r.order:range count r.frames
 r
end
on set_value x do
 if x.frames
  fr.images: x.frames..copy[]
 end
end
on audio x do
 if !app.playing
  play[sound[random[extract data where name=x from sfx.value]]]
 end
end

on drag x do
 fri:frames[]
 o:if pointer.down x else pointer.prev-c.offset end
 t:toolval[]
 on commonTool disp do
  c.brush:t
  each f in fri
   c.paste[f]
   c.line[disp[o] disp[x]]
   f.paste[c.copy[]]
  end
 end
 if t~"pen"
  audio.pen
  c.brush:1
  each f in fri
   c.paste[f]
   c.line[o random[-1,0,1 2]+x]
   f.paste[c.copy[]]
  end
 elseif t~"eraser"   audio.erase c.pattern:0 commonTool[on _ x do x end] c.pattern:1
 elseif t~"sumi"     audio.wipe  commonTool[wiggle]
 elseif t~"stipple"  audio.stip  commonTool[wiggle]
 elseif t~"airbrush" audio.spray commonTool[wiggle]
 else
  audio.squeak
  pat:("%j" parse "{l}'pink':36,'cyan':39,'yellow':33,'mask':32{r}")[t]
  img:image[marker].map[1,0]
  s:img.size
  y:x-s/2
  each f in fri
   w:wiggle[y]
   f.paste[img.copy[].merge[f.copy[w s].map[1 dict 1 pat]] w 1]
  end
 end
end
on click x do
 backup[]
 drag[x]
end

on view do
 f:frames[]
 i:f[(count f)%sys.frame/5]
 trans_mask.toggle["transparent" trans.value]
 c.show:if trans.value "transparent" else "solid" end
 c.size:i.size
 c.pos:zone.pos+.5*zone.size-c.size
 c.paste[i]
end

{end}

{script:wigglyModal.1p}
