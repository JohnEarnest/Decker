{module:ajax}
description:"A Dangerous JS binding for making asynchronous HTTP requests."
version:1
{script}
on validate do
 if !sys.platform~"web" alert["the 'ajax' module only works in web-decker."]
 elseif !danger.js alert["the 'ajax' module requires the danger zone."]
 end
end

# for some applications it may be desirable to extend this to handle
# binary response data/payloads or to inform the caller if we get
# a response code other than "OK":

natives:danger.js["_=>({l}
 send:function send(verb,url,payload,callback){l}
  const x=new XMLHttpRequest()
  x.onreadystatechange=_=>{l}
   if(x.readyState==XMLHttpRequest.DONE&&x.status==200)callback(x.responseText)
  {r}
  x.open(verb,url)
  x.send(payload)
 {r},
{r})" 0]

module.send:on _ verb url payload target do
 validate[]
 on callback x do target.event["http_response" x] end
 natives.send[verb url payload callback]
end
{end}

{module:ls}
description:"A Dangerous JS binding for interacting with browser local storage."
version:1
{script}
on validate do
 if !sys.platform~"web" alert["the 'ls' module only works in web-decker."]
 elseif !danger.js alert["the 'ls' module requires the danger zone."]
 end
end

# in some browsers/contexts interacting with localstorage may not be
# allowed at all, so we need to guard against internal explosions:

natives:danger.js["_=>({l}
 set:function set(k,v){l}
  try{l}localStorage.setItem(k,v){r}catch(e){l}{r};return v
 {r},
 get:function get(k,v){l}
  try{l}return localStorage.getItem(k){r}catch(e){l}{r};return null
 {r},
{r})" 0]

module.set:on set key value do validate[] natives.set[key value] end
module.get:on get key       do validate[] natives.get[key      ] end
{end}

{module:kb}
description:"A Dangerous JS binding for obtaining raw keyboard events."
version:1
{script}
callbacks.down:on key_down deck key do deck.card.event["key_down" key] end
callbacks.up  :on key_up   deck key do deck.card.event["key_up"   key] end

# wrap the low-level input handlers for Decker itself and
# redirect a copy of these events to the active card if
# nothing else ought to have focus and we're in interact mode.
# some of this is rather brittle, because it relies on unstable
# internal functions comprising Web-Decker.

# these should only be installed once, or else they'll stack
# up nested handlers every time the module is initialized!

danger.js["callbacks=>{l}
 function nofocus(){l}
  return uimode=='interact'&&ms.type==null&&!wid.fv&&!wid.gv
 {r}
 if(!ext.kb){l}
  const key_down=document.body.onkeydown
  const key_up  =document.body.onkeyup
  document.body.onkeydown=e=>{l}
   key_down(e)
   if(nofocus())callbacks.down(deck,e.key)
  {r}
  document.body.onkeyup=e=>{l}
   key_up(e)
   if(nofocus())callbacks.up(deck,e.key)
  {r}
  ext.kb=1
 {r}
{r}" callbacks]
{end}

{module:bgm}
description:"A Dangerous JS binding for playing looped background music from external media files."
version:1
{script}
on validate do
 if !sys.platform~"web" alert["the 'bgm' module only works in web-decker."]
 elseif !danger.js alert["the 'bgm' module requires the danger zone."]
 end
end

# safari requires audio playback be initiated in direct response to
# user action the first time it happens, so we play a dummy sound
# in response to a user's clicks.

# stash the Audio object in ext.bgm.a so that we can access it (and stop it)
# even if the deck is reloaded:

natives:danger.js["_=>{l}
 if(!ext.bgm){l}
  ext.bgm={l}init:0,a:null{r}
  document.body.addEventListener('mousedown',_=>{l}
   if(!ext.bgm.init){l}
    new Audio().play().catch(e=>console.log('init',e))
    ext.bgm.init=1
   {r}
  {r})
 {r}

 function volume(x){l}if(ext.bgm.a)ext.bgm.a.volume=x{r}
 function stop(){l}if(ext.bgm.a)ext.bgm.a.pause(){r}
 function play(path){l}
  stop()
  ext.bgm.a=new Audio(path)
  ext.bgm.a.loop=true
  ext.bgm.a.play().catch(e=>{l}console.log(e){r})
 {r}

 return {l}play,stop,volume{r}
{r}" 0]

module.play  :on play path do validate[] natives.play[path]    end
module.stop  :on stop      do validate[] natives.stop[]        end
module.volume:on volume x  do validate[] natives.volume[0|1&x] end
{end}

