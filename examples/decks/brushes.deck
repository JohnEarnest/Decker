# parallel to stroke angle
brush[on dirSml delta do random[ds[16%  16*(heading delta)/2*pi]] end]
brush[on dirMed delta do random[dm[16%  16*(heading delta)/2*pi]] end]
brush[on dirLrg delta do random[dl[16%  16*(heading delta)/2*pi]] end]

# perpendicular to stroke angle
brush[on offSml delta do random[ds[16%4+16*(heading delta)/2*pi]] end]
brush[on offMed delta do random[dm[16%4+16*(heading delta)/2*pi]] end]
brush[on offLrg delta do random[dl[16%4+16*(heading delta)/2*pi]] end]

# hatching
brush[on hatch delta do random[dl[16%random[2,-2]+16*(heading delta)/2*pi]] end]

# sparse directional stipples
brush[on sparse  delta do if  random[2] random[sl[16%16*(heading delta)/2*pi]] end end]
brush[on xsparse delta do if !random[4] random[sl[16%16*(heading delta)/2*pi]] end end]

{end}

{module:stepBrushes}
description:"Brushes that are feets"
version:1
{script}
on steps feet do
 flip each foot in "%J" parse feet
  image[64,64].paste[foot 32,16],
  image[64,64].paste[foot 32,32]
 end
end
