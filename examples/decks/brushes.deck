{module:screenPrints}
description:"Ordered-Dither brushes resembling screen prints"
version:1.1
{script}
smallBrush:image["%%IMG0AAgACMeDAQEBg8f/"]
largeBrush:image["%%IMG3ACAAIAKMjwbLmw9PmzNGiquVubvthYAlilBZJiiKrKnivrE8h0GN5/rO9/4PzN2CMGDxx/G1kEveaUcSgmqb5KqqemGfFGwB"]
patLight  :image[largeBrush.size+8].merge[image["%%IMG0AAgACIgiiCKIIogi"]] # pattern 12
patMed    :image[largeBrush.size+8].merge[image["%%IMG0AAgACKpVqlWqVapV"]] # pattern 13
patDark   :image[largeBrush.size+8].merge[image["%%IMG0AAgACHfdd9133Xfd"]] # pattern  9
brush[on screenSmlLight d f p do b:smallBrush b.copy[].merge[patLight.copy[8%p b.size]] end]
brush[on screenLrgLight d f p do b:largeBrush b.copy[].merge[patLight.copy[8%p b.size]] end]
brush[on screenSmlMed   d f p do b:smallBrush b.copy[].merge[patMed  .copy[8%p b.size]] end]
brush[on screenLrgMed   d f p do b:largeBrush b.copy[].merge[patMed  .copy[8%p b.size]] end]
brush[on screenSmlDark  d f p do b:smallBrush b.copy[].merge[patDark .copy[8%p b.size]] end]
brush[on screenLrgDark  d f p do b:largeBrush b.copy[].merge[patDark .copy[8%p b.size]] end]
{end}

{contraption:imageEnum}
size:[100,100]
resizable:1
margin:[17,3,18,5]
description:"select from among enumerated image values."
version:1.1
script:"imageEnum.0p"
template:"on change img index do\n \nend"
attributes:{"name":["options"],"label":["Options"],"type":["rich"]}
{widgets}
s:{"type":"slider","size":[100,100],"pos":[0,0],"interval":[0,0],"format":"","style":"compact"}
c:{"type":"canvas","size":[70,96],"pos":[15,2],"locked":1,"border":0,"scale":1}
o:{"type":"field","size":[44,53],"pos":[129,-20],"locked":1,"show":"none"}

{script:imageEnum.0p}
on get_options do o.value end
on set_options x do o.value:x view[] x end
on get_value do o.images[s.value] end
on get_valueIndex do s.value end
on set_valueIndex x do s.value:x view[] x end

on view do
 im:o.images
 i:im[s.value]
 s.interval:0,0|(count im)-1
 s.locked:card.locked
 s.show  :card.show
 c.show  :card.show
 c.clear[]
 c.paste[i .5*c.size-i.size 1]
end

on change do
 card.event["change" get_value[] s.value]
 view[]
end
{end}

