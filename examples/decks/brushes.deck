local dist  :0
local parity:0
on footbrush delta spacing images do
 dist:dist-mag delta
 if dist<0
  parity:!parity
  dist:spacing+random[10]
  random[images[parity]].copy[].rotate[heading delta]
 end
end

brush[on toebeans       delta do footbrush[delta 64 beans      ] end]
brush[on chickenscratch delta do footbrush[delta 75 chickenpaws] end]
