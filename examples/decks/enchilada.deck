{script:Draggables.0}
on overlaps a b do
 min(a.pos<b.pos+b.size),b.pos<a.pos+a.size
end

on inside a b do
 min(a.pos>b.pos),(a.pos+a.size)<b.pos+b.size
end
{end}

{script:Draggables.1}
on drag do
 c:container
 me.pos:(c.pos+c.size-me.size)&c.pos|me.pos
end
{end}

{script:Draggables.2}
on click do
 held.value:1
end

on drag do
 target.show:if overlaps[me target] "invert" else "solid" end
end

on release do
 held.value:0
 if overlaps[me target]
  alert["you dropped me on the target!"]
 end
end
{end}

{script:Draggables.3}
on drag do
 target.show:if inside[me target] "invert" else "solid" end
end

on release pos do
 if inside[me target]
  alert["you dropped me inside the target!"]
 else
  me.pos:pos # snap back to original position
 end
end
{end}

